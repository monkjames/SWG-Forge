{
    "name": "flr-viewer",
    "displayName": "SWG FLR Viewer",
    "description": "Visual floor mesh viewer for SWG building cells",
    "version": "1.0.0",
    "publisher": "swgemu",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": ["Other"],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "flrViewer.flrFile",
                "displayName": "FLR Viewer",
                "selector": [
                    {
                        "filenamePattern": "*.flr"
                    }
                ],
                "priority": "default"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/vscode": "^1.74.0",
        "typescript": "^5.0.0"
    }
}
