{
    "name": "tre-viewer",
    "displayName": "SWG TRE Viewer",
    "description": "Read-only viewer for SWG TRE archive files - browse contained files and folder structure",
    "version": "1.0.0",
    "publisher": "swgemu",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": ["Other"],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "treViewer.treFile",
                "displayName": "TRE Viewer",
                "selector": [
                    {
                        "filenamePattern": "*.tre"
                    }
                ],
                "priority": "default"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/vscode": "^1.74.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@swgemu/core": "file:../core"
    }
}
