{
    "name": "swg-spawn-visualizer",
    "displayName": "SWG Spawn Visualizer",
    "description": "Visualize planet spawn maps - see creature distribution, find missing mobs, spot cross-planet errors",
    "version": "1.0.0",
    "publisher": "swgemu",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:spawnVisualizer.open"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "spawnVisualizer.open",
                "title": "Open Spawn Visualizer",
                "category": "SWG Forge"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/vscode": "^1.74.0",
        "typescript": "^5.0.0"
    }
}
