{
    "name": "bdb-viewer",
    "displayName": "SWG BDB Viewer",
    "description": "Read-only viewer for SWGEmu Berkeley DB databases with field decoding",
    "version": "1.0.0",
    "publisher": "swgemu",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": ["Other"],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "bdbViewer.bdbFile",
                "displayName": "BDB Viewer",
                "selector": [
                    {
                        "filenamePattern": "*.db"
                    }
                ],
                "priority": "default"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/vscode": "^1.74.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@swgemu/core": "file:../core"
    }
}
