{
    "name": "object-creator",
    "displayName": "SWG Object Creator",
    "description": "Create new SWG objects from existing appearance chains",
    "version": "1.0.0",
    "publisher": "swgemu",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:objectCreator.open"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "objectCreator.open",
                "title": "Object Creator",
                "category": "SWG"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "@swgemu/core": "file:../core"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/vscode": "^1.74.0",
        "typescript": "^5.0.0"
    }
}
